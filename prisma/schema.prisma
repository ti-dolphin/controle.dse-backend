generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("HOMOLOG_DATABASE_URL")
}

model motivo_perdido {
  id           Int            @id @default(autoincrement())
  nome         String         @db.VarChar(150)
  ORDEMSERVICO ORDEMSERVICO[]
}

model produtos {
  ID                    Int                    @id
  cod_coligada          Int                    @db.SmallInt
  codigo                String?                @db.VarChar(30)
  nome_fantasia         String?                @db.VarChar(120)
  descricao             String?                @db.VarChar(240)
  inativo               Int?                   @db.SmallInt
  criado_por            String?                @db.VarChar(50)
  data_criacao          DateTime?              @db.DateTime(0)
  modificado_por        String?                @db.VarChar(50)
  data_modificacao      DateTime?              @db.DateTime(0)
  ultimo_nivel          Int                    @default(0) @db.SmallInt
  unidade               String?                @db.VarChar(10)
  familia               String?                @db.VarChar(9)
  quantidade_estoque    Int?                   @default(0)
  quantidade_reservada  Int?                   @default(0)
  quantidade_disponivel Int?
  WEB_REQUISICAO_ITEMS  WEB_REQUISICAO_ITEMS[]
  web_anexos_produto    web_anexos_produto[]
  web_items_cotacao     web_items_cotacao[]
  web_patrimonio        web_patrimonio[]
}

model web_anexos_os {
  id_anexo_os  Int          @id @default(autoincrement())
  codos        Int
  nome_arquivo String       @db.Text
  arquivo      String       @db.Text
  criado_em    DateTime?    @db.DateTime(0)
  criado_por   Int?
  pessoa       PESSOA?      @relation(fields: [criado_por], references: [CODPESSOA], onDelete: NoAction, onUpdate: NoAction, map: "created_by")
  ordemservico ORDEMSERVICO @relation(fields: [codos], references: [CODOS], onDelete: Cascade, map: "web_anexos_os_ibfk_1")

  @@index([codos], map: "web_anexos_os_ibfk_1")
  @@index([criado_por], map: "pessoa_criado_por_idx")
}

model web_checklist_movimentacao {
  id_checklist_movimentacao        Int                                @id @default(autoincrement())
  id_movimentacao                  Int
  data_criacao                     DateTime                           @default(now()) @db.Timestamp(0)
  realizado                        Boolean?                           @default(false)
  data_realizado                   DateTime?                          @db.Timestamp(0)
  aprovado                         Boolean?                           @default(false)
  data_aprovado                    DateTime?                          @db.Timestamp(0)
  observacao                       String?                            @db.Text
  reprovado                        Int?                               @db.TinyInt
  movimentacao_patrimonio          web_movimentacao_patrimonio        @relation(fields: [id_movimentacao], references: [id_movimentacao], onDelete: Cascade, map: "web_checklist_movimentacao_ibfk_1")
  web_items_checklist_movimentacao web_items_checklist_movimentacao[]

  @@index([id_movimentacao], map: "id_movimentacao")
}

model web_classificacao_fiscal {
  id_classificao_fiscal Int           @id
  nome                  String        @db.VarChar(50)
  web_cotacao           web_cotacao[]
}

model web_cotacao {
  id_cotacao               Int                       @id @default(autoincrement())
  id_requisicao            Int
  fornecedor               String                    @db.VarChar(255)
  data_cotacao             DateTime?                 @default(now()) @db.DateTime(0)
  observacao               String?                   @db.Text
  descricao                String?                   @db.Text
  cnpj_faturamento         String?                   @default("04016153000188") @db.VarChar(255)
  valor_frete              Decimal?                  @db.Decimal(10, 2)
  id_tipo_frete            Int?
  id_classificacao_fiscal  Int?
  cnpj_fornecedor          String?                   @db.VarChar(255)
  criado_por               Int?
  id_condicao_pagamento    Int?
  valor_total_itens        Decimal?                  @db.Decimal(10, 0)
  valor_total              Decimal?                  @db.Decimal(10, 0)
  web_anexo_cotacao        web_anexo_cotacao[]
  web_classificacao_fiscal web_classificacao_fiscal? @relation(fields: [id_classificacao_fiscal], references: [id_classificao_fiscal], onDelete: NoAction, onUpdate: NoAction, map: "classificacao_fiscal")
  pessoa                   PESSOA?                   @relation(fields: [criado_por], references: [CODPESSOA], onDelete: NoAction, onUpdate: NoAction, map: "criado_por")
  web_tipo_frete           web_tipo_frete?           @relation(fields: [id_tipo_frete], references: [id_tipo_frete], onDelete: NoAction, onUpdate: NoAction, map: "tipo_frete")
  web_requisicao           WEB_REQUISICAO            @relation(fields: [id_requisicao], references: [ID_REQUISICAO], onDelete: Cascade, map: "web_cotacao_ibfk_1")
  web_items_cotacao        web_items_cotacao[]

  @@index([id_classificacao_fiscal], map: "classificacao_fiscal_idx")
  @@index([criado_por], map: "criado_por_idx")
  @@index([id_requisicao], map: "id_requisicao")
  @@index([id_tipo_frete], map: "tipo_frete_idx")
}

model web_familia_tipo {
  familia                   String              @db.Char(9)
  id_tipo_requisicao        Int
  id_tipo_requisicao_backup Int?
  web_tipo_requisicao       web_tipo_requisicao @relation(fields: [id_tipo_requisicao], references: [id_tipo_requisicao], onDelete: Cascade, map: "fk_tipo_requisicao")

  @@id([familia, id_tipo_requisicao])
  @@index([id_tipo_requisicao], map: "fk_tipo_requisicao")
}

model web_items_checklist_movimentacao {
  id_item_checklist_movimentacao Int                        @id @default(autoincrement())
  id_checklist_movimentacao      Int
  nome_item_checklist            String                     @db.VarChar(255)
  arquivo                        String?                    @db.VarChar(255)
  problema                       Boolean?                   @default(false)
  valido                         Boolean?                   @default(true)
  observacao                     String?                    @db.Text
  web_checklist_movimentacao     web_checklist_movimentacao @relation(fields: [id_checklist_movimentacao], references: [id_checklist_movimentacao], onDelete: Cascade, map: "web_items_checklist_movimentacao_ibfk_1")

  @@index([id_checklist_movimentacao], map: "web_items_checklist_movimentacao_ibfk_1")
}

model web_items_checklist_tipo {
  id_items_checklist_tipo Int                 @id @default(autoincrement())
  id_tipo_patrimonio      Int
  nome_item_checklist     String              @db.VarChar(255)
  created_at              DateTime            @default(now()) @db.Timestamp(0)
  updated_at              DateTime            @default(now()) @db.Timestamp(0)
  web_tipo_patrimonio     web_tipo_patrimonio @relation(fields: [id_tipo_patrimonio], references: [id_tipo_patrimonio], onDelete: Cascade, map: "web_items_checklist_tipo_ibfk_1")

  @@index([id_tipo_patrimonio], map: "id_tipo_patrimonio")
}

model web_items_cotacao {
  id_item_cotacao       Int                   @id @default(autoincrement())
  id_cotacao            Int
  descricao_item        String                @db.VarChar(255)
  preco_unitario        Decimal               @default(0.00) @db.Decimal(10, 2)
  quantidade_solicitada Int                   @default(0)
  subtotal              Decimal?              @default(0.00) @db.Decimal(10, 2)
  id_item_requisicao    Int?
  observacao            String?               @db.Text
  ICMS                  Decimal?              @db.Decimal(10, 3)
  IPI                   Decimal?              @db.Decimal(10, 3)
  ST                    Decimal?              @db.Decimal(10, 3)
  quantidade_cotada     Int?
  id_produto            Int?
  indisponivel          Int?                  @db.TinyInt
  produtos              produtos?             @relation(fields: [id_produto], references: [ID], onDelete: NoAction, onUpdate: NoAction, map: "id_produto")
  WEB_REQUISICAO_ITEMS  WEB_REQUISICAO_ITEMS? @relation(fields: [id_item_requisicao], references: [id_item_requisicao], onDelete: Cascade, map: "item_requisicao_relacionado")
  web_cotacao           web_cotacao           @relation(fields: [id_cotacao], references: [id_cotacao], onDelete: Cascade, map: "web_items_cotacao_ibfk_1")

  @@index([id_cotacao], map: "id_cotacao")
  @@index([id_produto], map: "produto_cotado")
  @@index([id_item_requisicao], map: "item_requisicao_relacionado_idx")
}

model web_patrimonio {
  id_patrimonio               Int                           @id @default(autoincrement())
  nome                        String                        @db.Text
  data_compra                 DateTime?                     @db.Date
  nserie                      String?                       @db.VarChar(255)
  descricao                   String?                       @db.Text
  pat_legado                  String?                       @db.VarChar(100)
  tipo                        Int?
  ativo                       Int?                          @default(1) @db.TinyInt
  fabricante                  String?                       @db.Text
  valor_compra                Decimal?                      @db.Decimal(10, 2)
  id_produto                  Int?
  web_acessorio_patrimonio    web_acessorio_patrimonio[]
  web_anexos_patrimonio       web_anexos_patrimonio[]
  web_movimentacao_patrimonio web_movimentacao_patrimonio[]
  produtos                    produtos?                     @relation(fields: [id_produto], references: [ID], onDelete: NoAction, onUpdate: NoAction, map: "produto")
  web_tipo_patrimonio         web_tipo_patrimonio?          @relation(fields: [tipo], references: [id_tipo_patrimonio], onDelete: NoAction, onUpdate: NoAction, map: "tipo")

  @@index([id_produto], map: "produto_idx")
  @@index([tipo], map: "tipo_patrimonio_idx")
}

model web_seguidores_projeto {
  id_seguidor_projeto Int      @id @default(autoincrement())
  id_projeto          Int
  codpessoa           Int
  ativo               Boolean  @default(true)
  pessoa              PESSOA   @relation(fields: [codpessoa], references: [CODPESSOA], onUpdate: Restrict, map: "fk_seguidor_projeto_pessoa")
  projetos            PROJETOS @relation(fields: [id_projeto], references: [ID], onDelete: Cascade, map: "fk_seguidor_projeto_projeto")

  @@index([codpessoa], map: "fk_seguidor_projeto_pessoa")
  @@index([id_projeto], map: "fk_seguidor_projeto_projeto")
}

model web_tipo_frete {
  id_tipo_frete Int           @id @default(autoincrement())
  nome          String        @db.VarChar(50)
  web_cotacao   web_cotacao[]
}

model web_tipo_patrimonio {
  id_tipo_patrimonio       Int                        @id @default(autoincrement())
  nome_tipo                String                     @db.VarChar(255)
  descricao_tipo           String?                    @db.Text
  periodicidade            Int?
  responsavel_tipo         Int?
  web_items_checklist_tipo web_items_checklist_tipo[]
  web_patrimonio           web_patrimonio[]
}

model web_tipo_requisicao {
  id_tipo_requisicao Int                @id @default(autoincrement())
  nome_tipo          String             @db.VarChar(255)
  WEB_REQUISICAO     WEB_REQUISICAO[]
  web_familia_tipo   web_familia_tipo[]
}

model web_alteracao_req_status {
  id_alteracao          Int                   @id @default(autoincrement())
  id_status_requisicao  Int
  alterado_por          Int
  data_alteracao        DateTime              @db.DateTime(0)
  id_requisicao         Int?
  justificativa         String?               @db.Text
  id_status_anterior    Int
  pessoa                PESSOA                @relation(fields: [alterado_por], references: [CODPESSOA], onDelete: Cascade, map: "alterado_por")
  web_requisicao        WEB_REQUISICAO?       @relation(fields: [id_requisicao], references: [ID_REQUISICAO], onDelete: Cascade, map: "id-requisicao")
  web_status_requisicao web_status_requisicao @relation(fields: [id_status_requisicao], references: [id_status_requisicao], onDelete: Cascade, map: "web_alteracao_req_status_ibfk_1")

  @@index([alterado_por], map: "alterado_por_idx")
  @@index([id_requisicao], map: "id-requisicao_idx")
  @@index([id_status_requisicao], map: "id_status_requisicao")
}

model web_anexo_cotacao {
  id_anexo_cotacao Int         @id @default(autoincrement())
  id_cotacao       Int
  nome_arquivo     String      @db.VarChar(255)
  url              String      @db.Text
  web_cotacao      web_cotacao @relation(fields: [id_cotacao], references: [id_cotacao], onDelete: Cascade, map: "fk_id_cotacao")

  @@index([id_cotacao], map: "fk_id_cotacao")
}

model web_condicao_pagamento {
  id_condicao_pagamento Int     @id @default(autoincrement())
  nome                  String? @db.Text
}

model web_kanban_requisicao {
  id_kanban_requisicao Int     @id @default(autoincrement())
  nome                 String? @db.VarChar(255)
}

model web_kanban_status_requisicao {
  id                   Int     @id
  id_kanban_requisicao Int
  id_status_requisicao Int
  perfil               String? @db.VarChar(45)
  nome_perfil          String? @db.VarChar(45)
  nome_status          String? @db.VarChar(45)
  nome_kanban          String? @db.VarChar(45)
}

model web_oc {
  codoc             Int       @id
  data_entrega_obra DateTime? @db.DateTime(0)
  status            String?   @db.VarChar(10)
}

model web_perfil_acao_status_requisicao {
  id_perfil_permissao_status Int     @id
  id_perfil_usuario          Int?
  id_status_requisicao       Int?
  acao                       Int?
  nome_perfil                String? @db.VarChar(45)
  nome_status                String? @db.VarChar(45)
}

model web_perfil_usuario {
  id_perfil_usuario Int      @id @default(autoincrement())
  nome              String   @db.VarChar(100)
  data_criacao      DateTime @default(now()) @db.Timestamp(0)
  data_atualizacao  DateTime @default(now()) @db.Timestamp(0)
  ativo             Boolean? @default(true)
}

model web_status_requisicao {
  id_status_requisicao                                                                  Int                        @id @default(autoincrement())
  nome                                                                                  String                     @db.VarChar(50)
  acao_posterior                                                                        String                     @db.Text
  etapa                                                                                 Int?
  acao_anterior                                                                         String?                    @db.Text
  id_escopo_requisicao                                                                  Int?
  ativo                                                                                 Int?                       @db.TinyInt
  WEB_REQUISICAO                                                                        WEB_REQUISICAO[]
  web_alteracao_req_status                                                              web_alteracao_req_status[]
  web_escopo_requisicao                                                                 web_escopo_requisicao?     @relation(fields: [id_escopo_requisicao], references: [id_escopo_requisicao], onDelete: NoAction, onUpdate: NoAction, map: "escopo")
  web_transicao_status_web_transicao_status_id_status_anteriorToweb_status_requisicao   web_transicao_status[]     @relation("web_transicao_status_id_status_anteriorToweb_status_requisicao")
  web_transicao_status_web_transicao_status_id_status_requisicaoToweb_status_requisicao web_transicao_status[]     @relation("web_transicao_status_id_status_requisicaoToweb_status_requisicao")

  @@index([acao_posterior(length: 255)], map: "idx_web_status_acao")
  @@index([id_escopo_requisicao], map: "escopo_idx")
}

model web_acessorio_patrimonio {
  id_acessorio_patrimonio         Int                               @id @default(autoincrement())
  nome                            String                            @db.Text
  id_patrimonio                   Int?
  descricao                       String?                           @db.Text
  web_patrimonio                  web_patrimonio?                   @relation(fields: [id_patrimonio], references: [id_patrimonio], onDelete: Restrict, onUpdate: Restrict, map: "web_acessorio_patrimonio_ibfk_1")
  web_anexos_acessorio_patrimonio web_anexos_acessorio_patrimonio[]

  @@index([id_patrimonio], map: "id_patrimonio")
}

model web_movimentacao_patrimonio {
  id_movimentacao            Int                          @id @default(autoincrement())
  data                       DateTime                     @db.DateTime(0)
  id_patrimonio              Int?
  id_projeto                 Int?
  id_responsavel             Int?
  id_ultima_movimentacao     Int?
  observacao                 String?                      @db.Text
  aceito                     Int?                         @default(0)
  web_anexos_movimentacao    web_anexos_movimentacao[]
  web_checklist_movimentacao web_checklist_movimentacao[]
  projetos                   PROJETOS?                    @relation(fields: [id_projeto], references: [ID], onDelete: Cascade, onUpdate: Restrict, map: "fk_projeto")
  pessoa                     PESSOA?                      @relation(fields: [id_responsavel], references: [CODPESSOA], onDelete: Cascade, onUpdate: Restrict, map: "fk_responsavel")
  web_patrimonio             web_patrimonio?              @relation(fields: [id_patrimonio], references: [id_patrimonio], onDelete: Cascade, onUpdate: Restrict, map: "web_movimentacao_patrimonio_ibfk_1")

  @@index([id_projeto], map: "fk_projeto")
  @@index([id_responsavel], map: "fk_responsavel")
  @@index([id_patrimonio], map: "movimentacao_patrimonio_ibfk_1")
}

model web_anexos_acessorio_patrimonio {
  id_anexo_acessorio_patrimonio Int                       @id @default(autoincrement())
  id_acessorio_patrimonio       Int?
  nome                          String                    @db.VarChar(255)
  arquivo                       String                    @db.VarChar(255)
  web_acessorio_patrimonio      web_acessorio_patrimonio? @relation(fields: [id_acessorio_patrimonio], references: [id_acessorio_patrimonio], onDelete: Cascade, onUpdate: Restrict, map: "web_anexos_acessorio_patrimonio_ibfk_1")

  @@index([id_acessorio_patrimonio], map: "anexo_acessorio_patrimonio_ibfk_1")
}

model web_anexos_item_requisicao {
  id_anexo_item_requisicao Int                  @id @default(autoincrement())
  arquivo                  String?              @db.LongText
  id_item_requisicao       Int
  nome_arquivo             String?              @db.LongText
  WEB_REQUISICAO_ITEMS     WEB_REQUISICAO_ITEMS @relation(fields: [id_item_requisicao], references: [id_item_requisicao], onDelete: Cascade, onUpdate: NoAction, map: "id_item")

  @@index([id_item_requisicao], map: "id_item_idx")
}

model web_anexos_movimentacao {
  id_anexo_movimentacao       Int                          @id @default(autoincrement())
  arquivo                     String                       @db.Text
  nome_arquivo                String?                      @db.VarChar(255)
  id_movimentacao             Int?
  web_movimentacao_patrimonio web_movimentacao_patrimonio? @relation(fields: [id_movimentacao], references: [id_movimentacao], onDelete: Cascade, onUpdate: Restrict, map: "web_anexos_movimentacao_ibfk_1")

  @@index([id_movimentacao], map: "anexo_movimentacao_ibfk_1")
}

model web_anexos_patrimonio {
  id_anexo_patrimonio Int             @id @default(autoincrement())
  arquivo             String          @db.Text
  nome_arquivo        String?         @db.VarChar(255)
  id_patrimonio       Int?
  web_patrimonio      web_patrimonio? @relation(fields: [id_patrimonio], references: [id_patrimonio], onDelete: Cascade, onUpdate: Restrict, map: "web_anexos_patrimonio_ibfk_1")

  @@index([id_patrimonio], map: "anexo_patrimonio_ibfk_1")
}

model web_anexos_requisicao {
  id_requisicao Int
  arquivo       String?   @db.Text
  nome_arquivo  String?   @db.VarChar(100)
  id            Int       @id @default(autoincrement())
  criado_por    Int?
  criado_em     DateTime? @db.DateTime(0)
  PESSOA        PESSOA?   @relation(fields: [criado_por], references: [CODPESSOA], onDelete: NoAction, onUpdate: NoAction, map: "criado_por_fk")

  @@index([criado_por], map: "criado_por_fk_idx")
  @@index([criado_por], map: "fk_criado_por_idx")
}

model web_transicao_status {
  id_transicao                                                                           Int                   @id @default(autoincrement())
  id_status_anterior                                                                     Int
  id_status_requisicao                                                                   Int
  nome_transicao                                                                         String                @db.VarChar(100)
  ativo                                                                                  Boolean?              @default(true)
  web_status_requisicao_web_transicao_status_id_status_anteriorToweb_status_requisicao   web_status_requisicao @relation("web_transicao_status_id_status_anteriorToweb_status_requisicao", fields: [id_status_anterior], references: [id_status_requisicao], onUpdate: Restrict, map: "web_transicao_status_ibfk_1")
  web_status_requisicao_web_transicao_status_id_status_requisicaoToweb_status_requisicao web_status_requisicao @relation("web_transicao_status_id_status_requisicaoToweb_status_requisicao", fields: [id_status_requisicao], references: [id_status_requisicao], onUpdate: Restrict, map: "web_transicao_status_ibfk_2")

  @@unique([id_status_anterior, id_status_requisicao], map: "id_status_anterior")
  @@index([id_status_requisicao], map: "id_status_requisicao")
}

model web_email_logs {
  id_log          Int     @id @default(autoincrement())
  erro            Int?    @db.TinyInt
  sucesso         Int?    @db.TinyInt
  destinatario    String? @db.VarChar(50)
  assunto         String? @db.Text
  id_destinatario Int?
  error_message   String? @db.Text
  pessoa          PESSOA? @relation(fields: [id_destinatario], references: [CODPESSOA], onDelete: NoAction, onUpdate: NoAction, map: "destinatario")

  @@index([id_destinatario], map: "destinatario_idx")
}

model web_verificao_ordemservico_logs {
  id_verificacao   Int       @id @default(autoincrement())
  sucesso          Int?      @db.TinyInt
  erro             Int?      @db.TinyInt
  data_verificacao DateTime? @db.DateTime(0)
}

model web_comentarios_requsicao {
  id_comentario_requisicao Int             @id @default(autoincrement())
  id_requisicao            Int?
  criado_por               Int
  data_criacao             DateTime?       @db.DateTime(0)
  data_alteracao           DateTime?       @db.DateTime(0)
  descricao                String?         @db.Text
  PESSOA                   PESSOA          @relation(fields: [criado_por], references: [CODPESSOA], onDelete: NoAction, onUpdate: NoAction, map: "fk_criado_por")
  web_requisicao           WEB_REQUISICAO? @relation(fields: [id_requisicao], references: [ID_REQUISICAO], map: "fk_id_requisicao")

  @@index([id_requisicao], map: "REQUISICAO")
  @@index([criado_por], map: "fk_criado_por_idx")
}

model web_escopo_requisicao {
  id_escopo_requisicao  Int                     @id @default(autoincrement())
  nome                  String                  @db.VarChar(50)
  descricao             String?                 @db.VarChar(255)
  web_status_requisicao web_status_requisicao[]
}

model web_anexos_produto {
  id_anexo_produto Int       @id @default(autoincrement())
